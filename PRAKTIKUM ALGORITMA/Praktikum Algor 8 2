R = int(input("masukkan angka : "))
L = int(input("masukkan pangkat : "))

def pangkat(R, L):
 if L>=1:
     return R* pangkat(R, L-1)
 return R

hasil = pangkat(R, L)
print(f"hasilnya adalah : {hasil} ")
